{
    "name": "NBWC-Action-Map",
    "private": true,
    "version": "0.0.0",
    "description": "An Action/Event Map for the National Black Worker Center",
    "author": "David Arvelo <david@radica.ly> (https://github.com/Radica)",
    "main": "index.ts",
    "engines": {
        "node": ">=10",
        "npm": ">=6"
    },
    "sideEffects": [
        "*.css",
        "*.scss",
        "*.sass"
    ],
    "scripts": {
        "dev": "cross-env NODE_ENV=development NODE_HOST=0.0.0.0 PORT=3000 DOTENV_CONFIG_PATH=.env.dev nodemon --exec ts-node -r dotenv/config --files .",
        "start": "cross-env NODE_ENV=production NODE_HOST=0.0.0.0 PORT=8080 DOTENV_CONFIG_PATH=.env ts-node -r dotenv/config --transpile-only --files .",
        "console": "cross-env NODE_ENV=development DOTENV_CONFIG_PATH=.env.dev ts-node -r dotenv/config",
        "build": "npm run clean:build && cross-env NODE_ENV=production DOTENV_CONFIG_PATH=.env webpack --progress --hide-modules --config ./tools/webpack/config.babel.js",
        "analyze": "npm run clean:build && cross-env NODE_ENV=analyze webpack -p --progress --hide-modules --config ./tools/webpack/config.babel.js",
        "type-check": "tsc --noEmit",
        "type-check:watch": "npm run type-check --watch",
        "lint": "run-p lint:*",
        "lint:code": "eslint --fix . --ext .js,.jsx,.ts,.tsx",
        "lint:style": "stylelint --fix \"./**/*.css\" \"./**/*.scss\" \"./**/*.sass\"",
        "test": "cross-env NODE_ENV=test jest",
        "test:watch": "npm run test --watch",
        "test:cov": "npm run test --coverage",
        "test:update": "npm run test -- -u",
        "clean": "run-p clean:*",
        "clean:build": "rimraf ./public/assets",
        "clean:cov": "rimraf ./coverage",
        "coveralls": "cat ./coverage/lcov.info | coveralls",
        "dc": "docker-compose -f docker-compose.yml",
        "docker": "npm run dc up",
        "dc:dev": "docker-compose -f docker-compose.yml -f docker-compose.dev.yml",
        "docker:dev": "npm run dc:dev up"
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged",
            "pre-push": "npm run test"
        }
    },
    "lint-staged": {
        "*.{ts,tsx,js,jsx}": "eslint --fix",
        "*.{css,scss,sass}": "stylelint --fix",
        "*.{json,md}": "prettier --write"
    },
    "babel": {
        "presets": [
            [
                "@babel/env",
                {
                    "useBuiltIns": "usage",
                    "corejs": "3.0.0"
                }
            ],
            "@babel/typescript",
            "@babel/react"
        ],
        "plugins": [
            "react-hot-loader/babel",
            "@loadable/babel-plugin",
            "@babel/plugin-proposal-class-properties",
            "@babel/plugin-syntax-dynamic-import"
        ],
        "env": {
            "production": {
                "plugins": [
                    "transform-remove-console"
                ]
            }
        }
    },
    "eslintConfig": {
        "parser": "@typescript-eslint/parser",
        "plugins": [
            "@typescript-eslint",
            "testing-library",
            "jest-dom",
            "prettier"
        ],
        "extends": [
            "airbnb",
            "airbnb/hooks",
            "plugin:@typescript-eslint/recommended",
            "plugin:testing-library/recommended",
            "plugin:jest-dom/recommended",
            "plugin:prettier/recommended",
            "prettier/@typescript-eslint",
            "prettier/react"
        ],
        "settings": {
            "import/parsers": {
                "@typescript-eslint/parser": [
                    ".ts",
                    ".tsx"
                ]
            },
            "import/resolver": {
                "typescript": {},
                "webpack": {
                    "config": "./tools/webpack/config.babel.js"
                }
            }
        },
        "env": {
            "browser": true,
            "node": true,
            "es2020": true,
            "jest": true
        },
        "rules": {
            "global-require": "off",
            "no-console": "off",
            "no-underscore-dangle": "off",
            "react/prop-types": "off",
            "react-hooks/exhaustive-deps": "error",
            "react/jsx-props-no-spreading": "off",
            "react/jsx-filename-extension": [
                "error",
                {
                    "extensions": [
                        ".js",
                        ".jsx",
                        ".ts",
                        ".tsx"
                    ]
                }
            ],
            "import/extensions": "off",
            "import/no-extraneous-dependencies": [
                "error",
                {
                    "devDependencies": true
                }
            ],
            "prettier/prettier": [
                "error",
                {
                    "singleQuote": true,
                    "tabWidth": 4,
                    "semi": true
                }
            ],
            "@typescript-eslint/ban-ts-ignore": "off",
            "@typescript-eslint/camelcase": "off",
            "@typescript-eslint/no-explicit-any": "off",
            "@typescript-eslint/no-use-before-define": "off",
            "@typescript-eslint/explicit-function-return-type": "off",
            "@typescript-eslint/no-var-requires": "off"
        },
        "globals": {
            "__CLIENT__": true,
            "__SERVER__": true,
            "__DEV__": true,
            "__API_BASEURL__": true,
            "__MAPBOX_ACCESS_TOKEN__": true
        }
    },
    "eslintIgnore": [
        "public/assets"
    ],
    "prettier": {
        "trailingComma": "es5",
        "tabWidth": 4,
        "semi": true,
        "singleQuote": true
    },
    "stylelint": {
        "plugins": [
            "stylelint-scss",
            "stylelint-prettier"
        ],
        "extends": [
            "stylelint-config-standard",
            "stylelint-config-recommended-scss",
            "stylelint-config-prettier"
        ],
        "rules": {
            "string-quotes": "single",
            "prettier/prettier": true,
            "selector-pseudo-class-no-unknown": [
                true,
                {
                    "ignorePseudoClasses": [
                        "global",
                        "local"
                    ]
                }
            ]
        },
        "ignoreFiles": [
            "public/assets/**/*.css",
            "coverage/**/*.css"
        ]
    },
    "jest": {
        "preset": "ts-jest",
        "setupFilesAfterEnv": [
            "<rootDir>/tools/jest/setup.ts"
        ],
        "globals": {
            "__DEV__": true
        },
        "collectCoverageFrom": [
            "src/actions/**/*.ts",
            "src/reducers/**/*.ts",
            "src/app/**/*.tsx",
            "src/pages/**/*.tsx",
            "src/components/**/*.tsx",
            "!src/actions/index.ts",
            "!src/reducers/index.ts",
            "!src/components/index.ts",
            "!src/pages/index.ts",
            "!src/pages/Home/index.js"
        ],
        "moduleNameMapper": {
            ".*\\.(css|scss|sass)$": "<rootDir>/tools/jest/styleMock.ts",
            ".*\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/tools/jest/assetMock.ts"
        }
    },
    "nodemonConfig": {
        "watch": [
            "src/server.tsx",
            "src/utils/renderHtml.ts"
        ]
    },
    "browserslist": [
        "> 1%",
        "last 2 versions"
    ],
    "dependencies": {
        "@babel/plugin-proposal-class-properties": "^7.8.3",
        "@babel/plugin-syntax-dynamic-import": "^7.8.3",
        "@babel/preset-env": "^7.9.5",
        "@babel/preset-react": "^7.9.4",
        "@babel/preset-typescript": "^7.9.0",
        "@babel/register": "^7.9.0",
        "@hot-loader/react-dom": "^16.13.0",
        "@loadable/babel-plugin": "^5.12.0",
        "@loadable/component": "^5.12.0",
        "@loadable/server": "^5.12.0",
        "@mapbox/mapbox-gl-geocoder": "^4.6.0",
        "asset-require-hook": "^1.2.0",
        "autoprefixer": "^9.7.6",
        "axios": "^0.19.2",
        "babel-plugin-dynamic-import-node": "^2.3.0",
        "babel-plugin-transform-remove-console": "^6.9.4",
        "body-parser": "^1.19.0",
        "chalk": "^4.0.0",
        "compression": "^1.7.4",
        "connected-react-router": "^6.8.0",
        "core-js": "^3.6.5",
        "cross-env": "^7.0.2",
        "css-modules-require-hook": "^4.2.3",
        "dotenv": "^8.2.0",
        "eslint-plugin-react-hooks": "^2.5.1",
        "express": "^4.17.1",
        "fetch-jsonp": "^1.1.3",
        "helmet": "^3.22.0",
        "history": "^4.10.1",
        "hpp": "^0.2.3",
        "html-minifier": "^4.0.0",
        "mapbox-gl": "^1.10.0",
        "moment": "^2.27.0",
        "morgan": "^1.10.0",
        "node-sass": "^4.14.1",
        "normalize.css": "^8.0.1",
        "pg": "^8.0.2",
        "react": "^16.13.1",
        "react-dom": "^16.13.1",
        "react-helmet": "^6.0.0",
        "react-hot-loader": "^4.12.20",
        "react-mapbox-gl": "^4.8.3",
        "react-moment": "^0.9.7",
        "react-redux": "^7.2.0",
        "react-router": "^5.1.2",
        "react-router-config": "^5.1.1",
        "react-router-dom": "^5.1.2",
        "redux": "^4.0.5",
        "redux-axios-middleware": "^4.0.1",
        "redux-thunk": "^2.3.0",
        "reflect-metadata": "^0.1.13",
        "serialize-javascript": "^4.0.0",
        "serve-favicon": "^2.5.0",
        "ts-node": "^8.8.2",
        "typeorm": "^0.2.24"
    },
    "devDependencies": {
        "@babel/core": "^7.9.0",
        "@loadable/webpack-plugin": "^5.12.0",
        "@testing-library/jest-dom": "^5.5.0",
        "@testing-library/react": "^10.0.2",
        "@types/compression": "^1.7.0",
        "@types/express": "^4.17.4",
        "@types/geojson": "^7946.0.7",
        "@types/helmet": "^0.0.45",
        "@types/hpp": "^0.2.1",
        "@types/html-minifier": "^3.5.3",
        "@types/jest": "^25.2.1",
        "@types/loadable__component": "^5.10.0",
        "@types/loadable__server": "^5.12.1",
        "@types/mapbox-gl": "^1.9.1",
        "@types/morgan": "^1.9.0",
        "@types/node": "^13.13.1",
        "@types/react": "^16.9.34",
        "@types/react-dom": "^16.9.6",
        "@types/react-helmet": "^5.0.15",
        "@types/react-redux": "^7.1.7",
        "@types/react-router": "^5.1.5",
        "@types/react-router-config": "^5.0.1",
        "@types/react-router-dom": "^5.1.4",
        "@types/react-test-renderer": "^16.9.2",
        "@types/redux-mock-store": "^1.0.2",
        "@types/serialize-javascript": "^1.5.0",
        "@types/serve-favicon": "^2.5.0",
        "@types/vfile-message": "^2.0.0",
        "@typescript-eslint/eslint-plugin": "^2.28.0",
        "@typescript-eslint/parser": "^2.28.0",
        "babel-loader": "^8.1.0",
        "compression-webpack-plugin": "^5.0.0",
        "coveralls": "^3.0.11",
        "css-loader": "^3.5.2",
        "eslint": "^7.0.0",
        "eslint-config-airbnb": "^18.1.0",
        "eslint-config-prettier": "^6.10.1",
        "eslint-import-resolver-typescript": "^2.0.0",
        "eslint-import-resolver-webpack": "^0.12.1",
        "eslint-plugin-import": "^2.20.1",
        "eslint-plugin-jest-dom": "^2.0.1",
        "eslint-plugin-jsx-a11y": "^6.2.3",
        "eslint-plugin-prettier": "^3.1.3",
        "eslint-plugin-react": "^7.19.0",
        "eslint-plugin-testing-library": "^3.0.3",
        "file-loader": "^6.0.0",
        "fork-ts-checker-webpack-plugin": "^4.1.3",
        "geojson": "^0.5.0",
        "husky": "^4.2.5",
        "identity-obj-proxy": "^3.0.0",
        "image-webpack-loader": "^6.0.0",
        "jest": "^25.3.0",
        "lint-staged": "^10.1.4",
        "mini-css-extract-plugin": "^0.9.0",
        "nodemon": "^2.0.3",
        "npm-run-all": "^4.1.5",
        "optimize-css-assets-webpack-plugin": "^5.0.3",
        "pnp-webpack-plugin": "^1.6.4",
        "postcss": "^7.0.27",
        "postcss-loader": "^3.0.0",
        "prettier": "^2.0.4",
        "raf": "^3.4.1",
        "react-test-renderer": "^16.13.1",
        "redux-mock-store": "^1.5.4",
        "rimraf": "^3.0.2",
        "sass-loader": "^8.0.2",
        "source-map-loader": "^0.2.4",
        "stylelint": "^13.3.2",
        "stylelint-config-prettier": "^8.0.1",
        "stylelint-config-recommended-scss": "^4.2.0",
        "stylelint-config-standard": "^20.0.0",
        "stylelint-prettier": "^1.1.2",
        "stylelint-scss": "^3.17.0",
        "terser-webpack-plugin": "^4.1.0",
        "ts-jest": "^25.4.0",
        "typescript": "^3.8.3",
        "url-loader": "^4.1.0",
        "webpack": "^4.44.1",
        "webpack-bundle-analyzer": "^3.7.0",
        "webpack-cli": "^3.3.11",
        "webpack-dev-middleware": "^3.7.2",
        "webpack-hot-middleware": "^2.25.0",
        "webpack-manifest-plugin": "^2.2.0"
    }
}
